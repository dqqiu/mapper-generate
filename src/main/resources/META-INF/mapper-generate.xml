<?xml version="1.0" encoding="UTF-8"?>
<generate xmlns="http://www.spirit.org/mapper-generate-1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.spirit.org/mapper-generate-1.0
 mapper-generate-1.0.xsd">
	<database>
		<driver>com.mysql.jdbc.Driver</driver>
		<url>jdbc:mysql://127.0.0.1:3306/dictionary</url>
		<user>root</user>
		<password>root</password>
	</database>
	<modules>
		<model targetPackage="org.spirit.mapper.generate.model" objectNameSuffix="Model" />
		<vo targetPackage="org.spirit.mapper.generate.vo" objectNameSuffix="VO" serializable="true" />
		<mapper targetPackage="org.spirit.mapper.generate.mapper" extend="org.spirit.mapper.generate.mapper.BaseMapper"/>
		<xml targetPackage="org.spirit.mapper.generate.xml"/>
		<!--
		<service targetPackage="org.spirit.mapper.generate.service"></service>
		<controller targetPackage="org.spirit.mapper.generate.controller"></controller> -->
	</modules>
	<tables>
		<table tableName="dict_state" primaryKey="cd,dscp"/>
		<table tableName="dict_province"  primaryKey="id" />
	</tables>
	<output>D:/generate/code</output>
</generate>